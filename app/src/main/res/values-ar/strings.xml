    <!-- Title of the SettingsActivity -->
    <string name="title_activity_settings">الإعدادات</string>

    <!-- Used to open Settings Menu from MainActivity and DetailActivity -->
    <string name="action_settings">الإعدادات</string>

    <!-- - - - - - - - - - - - - - - - -
     - Secondary weather info labels   -
     - - - - - - - - - - - - - - - - - -->
    <string name="wind_label">الرياح</string>
    <string name="pressure_label">الضغط</string>
    <string name="humidity_label">الرطوبة</string>

    <!-- - - - - - - - - - - - - - - - -
      - Preference Labels              -
      - - - - - - - - - - - - - - - - - -->
    <string name="pref_location_label">الموقع</string>
    <string name="pref_units_label">نظام وحدات القياس</string>
    <string name="pref_units_label_metric">النظام المتري</string>
    <string name="pref_units_label_imperial">النظام الامبراطوري</string>
    <string name="pref_enable_notifications_label">تنبيهات حالة الطقس</string>
    <string name="pref_enable_notifications_enabled">مفعل</string>
    <string name="pref_enable_notifications_not_enabled">غير مفعل</string>
    <!-- - - - - - - - - - - - - - - - -
      - Used by WeatherUtils           -
      - - - - - - - - - - - - - - - - -->

    <!-- HighLow Temperature format -->
    <string name="high_low_temperature"> \u200F
        \u00B0<xliff:g id="high_temp">%1$1.0f</xliff:g> /
        \u00B0<xliff:g id="low_temp">%2$1.0f</xliff:g>
    </string>

    <!-- Temperature format -->
    <string name="format_temperature">
        <xliff:g id="temp">%1.0f</xliff:g>\u00B0
    </string>

    <!-- Wind in kph -->
    <string name="format_wind_kmh">
        <xliff:g id="speed">%1$1.0f</xliff:g> كم/س
        <xliff:g id="direction">%2$s</xliff:g>
    </string>

    <!-- Wind in mph -->
    <string name="format_wind_mph">
        <xliff:g id="speed">%1$1.0f</xliff:g> م/س
        <xliff:g id="direction">%2$s</xliff:g>
    </string>

    <!-- Pressure format -->
    <string name="format_pressure">
        <xliff:g id="pressure">%1.0f</xliff:g> هكتوبسكال
    </string>

    <!-- Humidity format -->
    <string name="format_humidity">
        <xliff:g id="humidity">%1.0f</xliff:g> %%
    </string>

    <!-- - - - - - - - - - - - - - - - -
      - Wind Directions                -
      - - - - - - - - - - - - - - - - -->
    <string name="unknown_direction">غير معروف</string>
    <string name="north">شمال</string>
    <string name="east">شرق</string>
    <string name="west">غرب</string>
    <string name="south">جنوب</string>
    <string name="north_east">شمال شرق</string>
    <string name="south_east">جنوب شرق</string>
    <string name="north_west">شمال غرب</string>
    <string name="south_west">جنوب غرب</string>

    <!-- - - - - - - - - - - - - - -
      - Used by CustomDateUtils          -
      - - - - - - - - - - - - - - -->

    <!-- For labelling today -->
    <string name="today">اليوم</string>

    <!-- For labelling tomorrow -->
    <string name="tomorrow">غداً</string>

    <!-- - - - - - - - - - - - - - - - - -
     - Used for Accessibility purposes   -
     - - - - - - - - - - - - - - - - - -->
    <string name="access_forecast">حالة الطقس: <xliff:g id="condition">%1$s</xliff:g></string>
    <string name="access_high_temp">درجة الحرارة العظمى: <xliff:g id="high">%1$s</xliff:g></string>
    <string name="access_low_temp">درجة الحرارة الصغرى: <xliff:g id="low">%1$s</xliff:g></string>
    <string name="access_high_low_temp">
        درجة الحرارة العظمى: <xliff:g id="high">%1$1.0f</xliff:g>\u00B0 /
        درجة الحرارة الصغرى: <xliff:g id="low">%2$1.0f</xliff:g>\u00B0
    </string>
    <string name="access_humidity">الرطوبة: <xliff:g id="humidity">%1$s</xliff:g></string>
    <string name="access_pressure">الضغط الجوي: <xliff:g id="pressure">%1$s</xliff:g></string>
    <string name="access_wind">سرعة واتجاه الرياح: <xliff:g id="wind">%1$s</xliff:g></string>

    <!-- - - - - - - - - - - - - - - - - -
      - Used by NotificationUtils        -
      - - - - - - - - - - - - - - - - - -->

    <!-- Notification channel name for weather status updates channel -->
    <string name="weather_notification_channel_name">حالة الطقس</string>

    <!-- Notification channel description for weather status updates channel -->
    <string name="weather_notification_channel_description">
        هذه القناة تستخدم لإظهار تنبيهات حالة الطقس بعد تحديث البيانات من الانترنت مرة كل يوم
    </string>

    <!-- The format of the notification we show when the weather is updated -->
    <string name="format_notification">
        حالة الطقس: <xliff:g id="condition">%1$s</xliff:g> -
        الحرارة العظمى:<xliff:g id="high">%2$1.0f</xliff:g>
        الحرارة الصغرى:<xliff:g id="low">%3$1.0f</xliff:g>
    </string>

    <!-- Data syncing notification message to be used by sync foreground service notification -->
    <string name="sync_service_notification">جاري مزامنة البيانات …</string>


</resources>
